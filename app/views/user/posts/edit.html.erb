<%= form_for(@post, url:user_post_path) do |f| %>
    <h4>Title</h4>
    <%= f.text_field :title,autofocus: true, class:"form-control",placeholder:"title"%>
    <h4>Body</h4>
    <%= f.text_field :body,autofocus: true, class:"form-control",placeholder:"opinion"%>
    <h4><%= f.submit 'Update Book', class: "btn btn-default" %></h4>
<% end %>